  <title>{{title}}</title>
  <link rel="stylesheet" href="css/main.css">
  <link rel="stylesheet" type="text/css" href="https://cloud.typography.com/83898/7863772/css/fonts.css" />
</head>
<body class="{% if bodyClass %}{{bodyClass}}{% endif %}" id="midd-pagetop">
